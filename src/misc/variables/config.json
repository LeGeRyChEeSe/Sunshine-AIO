{
	"Sunshine": {
		"download_url": "https://github.com/LizardByte/Sunshine/releases/latest",
		"download_pattern": "sunshine-windows-installer",
		"downloaded_dir_path": "tools",
		"install_dir": "C:\\Program Files\\Sunshine",
		"service": "SunshineService",
		"settings_url": "https://localhost:47990/config"
	},
	"VirtualDisplayDriver": {
		"download_url": "https://github.com/VirtualDrivers/Virtual-Display-Driver/releases/latest",
		"download_pattern": "VDD.Control",
		"device_id": "Root\\IddSampleDriver",
		"device_id_new": "Root\\MttVDD",
		"driver_inf_old": "IddSampleDriver.inf",
		"driver_inf_new": "MttVDD.inf"
	},
	"SunshineVirtualMonitor": {
		"download_url": "https://github.com/Cynary/sunshine-virtual-monitor/archive/refs/heads/main.zip",
		"download_pattern": "sunshine-virtual-monitor-main.zip",
		"downloaded_dir_path": "tools\\sunshine-virtual-monitor-main"
	},
	"MultiMonitorTool": {
		"download_url": "https://www.nirsoft.net/utils/multimonitortool-x64.zip",
		"download_pattern": "multimonitortool-x64.zip",
		"downloaded_dir_path": "tools\\multimonitortool-x64"
	},
	"VsyncToggle": {
		"download_url": "https://github.com/xanderfrangos/vsync-toggle/releases/latest",
		"download_pattern": "vsynctoggle",
		"downloaded_dir_path": "tools"
	},
	"Playnite": {
		"download_url": "https://playnite.link/download/PlayniteInstaller.exe",
		"download_pattern": "PlayniteInstaller.exe",
		"downloaded_dir_path": "tools",
		"url": "playnite:"
	},
	"PlayniteWatcher": {
		"download_url": "https://github.com/Nonary/PlayNiteWatcher/releases/latest",
		"download_pattern": "PlayniteWatcher.zip",
		"downloaded_dir_path": "tools",
		"guide_url": "https://github.com/Nonary/PlayNiteWatcher#setup-instructions",
		"addon_url": "playnite://playnite/installaddon/SunshineAppExport"
	},
	"CommunityLibrary": {
		"repository_url": "https://github.com/LeGeRyChEeSe/sunshine-aio-library",
		"api_url": "https://api.github.com/repos/LeGeRyChEeSe/sunshine-aio-library",
		"cache_directory": "cache\\library",
		"sync_interval": 3600,
		"validation_enabled": true,
		"validation_level": "standard",
		"max_cache_size": 52428800,
		"max_cache_entries": 1000,
		"default_ttl": 86400,
		"timeout": 30
	}
}